<SCRIPT language=3DVBScript>=0A=
<!--=0A=
Dim =
tab1(10,10),tab2(10,10),njog,ntiro,jogada(3,2),pa(3,3),count,ttiros(6)=0A=
njog=3D0=0A=
ntiro=3D0=0A=
=0A=
Sub click(x,y)=0A=
	If ttiros(2)=3D0 Then=0A=
		MsgBox "Voc=EA afundou o hospital, n=E3o pode comandar a frota nem =
mais um minuto!"=0A=
		Exit Sub=0A=
	End If=0A=
=0A=
	If ntiro=3D0 Then ' primeiro lance=0A=
		njog=3Dnjog+1=0A=
	End If=0A=
	ntiro=3Dntiro+1=0A=
	jogada(ntiro,1)=3Dx=0A=
	jogada(ntiro,2)=3Dy=0A=
	document.all.item("c"&x&"l"&y).style.background=3D"lightblue"=0A=
	document.all.item("c"&x&"l"&y).innerHTML=3DCStr(njog)=0A=
	If ntiro=3D3 Then ' fazer a jogada=0A=
		Dim tiros(6),barco=0A=
		For i=3D0 To 6=0A=
			tiros(i)=3D0=0A=
		Next=0A=
		For i=3D1 To 3=0A=
			barco=3Dtab1(jogada(i,1),jogada(i,2))=0A=
			tiros(barco)=3Dtiros(barco)+1=0A=
			tab1(jogada(i,1),jogada(i,2))=3D0 ' resta =E1gua=0A=
		Next=0A=
		For i=3D1 To 6=0A=
			ttiros(i)=3Dttiros(i)-tiros(i)=0A=
		Next=0A=
		=0A=
		Dim ss=0A=
		If tiros(0)=3D3 Then=0A=
			ss=3D"Tudo na =E1gua."=0A=
		Else=0A=
			If tiros(1)>0 Then=0A=
				ss=3Dss&tiros(1)&" no porta-avi=F5es "&vbCrLf=0A=
				If ttiros(1)=3D0 Then=0A=
					ss=3Dss&"(Porta-avi=F5es afundado) "&vbCrLf=0A=
					Cores 1=0A=
				End If=0A=
			End If=0A=
			If tiros(2)>0 Then=0A=
				ss=3Dss&tiros(2)&" no hospital "&vbCrLf=0A=
				If ttiros(2)=3D0 Then=0A=
					ss=3Dss&"(Hospital afundado!) "&vbCrLf=0A=
					Cores 2=0A=
				End If=0A=
			End If=0A=
			If tiros(3)>0 Then=0A=
				ss=3Dss&tiros(3)&" no barco de 4 canos "&vbCrLf=0A=
				If ttiros(3)=3D0 Then=0A=
					ss=3Dss&"(Barco de 4 canos afundado) "&vbCrLf=0A=
					Cores 3=0A=
				End If=0A=
			End If=0A=
			If tiros(4)>0 Then=0A=
				ss=3Dss&tiros(4)&" num barco de 3 canos "&vbCrLf=0A=
				If ttiros(4)=3D0 Then=0A=
					ss=3Dss&"(Barcos de 3 canos ambos afundados) "&vbCrLf=0A=
					Cores 4=0A=
				End If=0A=
			End If=0A=
			If tiros(5)>0 Then=0A=
				ss=3Dss&tiros(5)&" num barco de 2 canos "&vbCrLf=0A=
				If ttiros(5)=3D0 Then=0A=
					ss=3Dss&"(Barcos de 2 canos todos afundados) "&vbCrLf=0A=
					Cores 5=0A=
				End If=0A=
			End If=0A=
			If tiros(6)>0 Then=0A=
				ss=3Dss&tiros(6)&" num submarino "&vbCrLf=0A=
				If ttiros(6)=3D0 Then=0A=
					ss=3Dss&"(Submarinos todos afundados) "&vbCrLf=0A=
					Cores 6=0A=
				End If=0A=
			End If=0A=
		End If=0A=
		res.innerHTML=3Dres.innerHTML&"<li>"&ss&"</li>"&vbCrLf=0A=
		MsgBox "Rajada "&njog&vbCrLf&ss=0A=
		ntiro=3D0=0A=
		If ttiros(1)=3D0 And ttiros(2)>0 And ttiros(3)=3D0 And ttiros(4)=3D0 =
And ttiros(5)=3D0 And ttiros(6)=3D0 Then=0A=
			MsgBox "Voc=EA levou "&njog&" rajadas a meter toda a frota inimiga no =
fundo do mar."=0A=
			njog=3D0=0A=
			GerarMapa=0A=
		End If=0A=
	End If	=0A=
End Sub=0A=
=0A=
Sub Cores (v)=0A=
	For i=3D1 To 10=0A=
		For j=3D1 To 10=0A=
			If tab2(j,i)=3Dv Then=0A=
				document.all.item("c"&j&"l"&i).style.background=3D"#FF7777"=0A=
			End If=0A=
		Next=0A=
	Next=0A=
End Sub=0A=
=0A=
Sub RodarPA(r)=0A=
	If r=3D1 Then=0A=
pa(1,1)=3D1=0A=
pa(1,2)=3D1=0A=
pa(1,3)=3D1=0A=
pa(2,1)=3D0=0A=
pa(2,2)=3D1=0A=
pa(2,3)=3D0=0A=
pa(3,1)=3D0=0A=
pa(3,2)=3D1=0A=
pa(3,3)=3D0=0A=
	ElseIf r=3D2 Then =0A=
pa(1,1)=3D0=0A=
pa(1,2)=3D1=0A=
pa(1,3)=3D0=0A=
pa(2,1)=3D0=0A=
pa(2,2)=3D1=0A=
pa(2,3)=3D0=0A=
pa(3,1)=3D1=0A=
pa(3,2)=3D1=0A=
pa(3,3)=3D1=0A=
	ElseIf r=3D3 Then=0A=
pa(1,1)=3D1=0A=
pa(1,2)=3D0=0A=
pa(1,3)=3D0=0A=
pa(2,1)=3D1=0A=
pa(2,2)=3D1=0A=
pa(2,3)=3D1=0A=
pa(3,1)=3D1=0A=
pa(3,2)=3D0=0A=
pa(3,3)=3D0=0A=
	ElseIf r=3D4 Then=0A=
pa(1,1)=3D0=0A=
pa(1,2)=3D0=0A=
pa(1,3)=3D1=0A=
pa(2,1)=3D1=0A=
pa(2,2)=3D1=0A=
pa(2,3)=3D1=0A=
pa(3,1)=3D0=0A=
pa(3,2)=3D0=0A=
pa(3,3)=3D1=0A=
	End If=0A=
End Sub=0A=
=0A=
Function Pode (x,y,r,n)=0A=
	Dim i,j=0A=
	If r=3D1 Then=0A=
		For i=3D-1 To 1=0A=
			If x+i>=3D1 And x+i<=3D10 Then=0A=
				For j=3D-1 To n=0A=
					If y+j>=3D1 And y+j<=3D10 Then=0A=
						If tab1(x+i,y+j)<>0 Then=0A=
							Pode=3DFalse=0A=
							Exit Function=0A=
						End If=0A=
					End If=0A=
				Next=0A=
			End If=0A=
		Next=0A=
	Else=0A=
		For i=3D-1 To 1=0A=
			If x+i>=3D1 And x+i<=3D10 Then=0A=
				For j=3D-1 To n=0A=
					If y+j>=3D1 And y+j<=3D10 Then=0A=
						If tab1(y+j,x+i)<>0 Then=0A=
							Pode=3DFalse=0A=
							Exit Function=0A=
						End If=0A=
					End If=0A=
				Next=0A=
			End If=0A=
		Next=0A=
	End If=0A=
	Pode=3DTrue=0A=
End Function=0A=
=0A=
Function Mete (x,y,r,n,v)=0A=
	Dim i,j=0A=
	If r=3D1 Then=0A=
		For j=3D0 To n-1=0A=
			tab1(x,y+j)=3Dv=0A=
		Next=0A=
	Else=0A=
		For j=3D0 To n-1=0A=
			tab1(y+j,x)=3Dv=0A=
		Next=0A=
	End If=0A=
End Function=0A=
=0A=
Sub Gerar (n,v)=0A=
	Dim x,y,r=0A=
	Do=0A=
		count=3Dcount+1=0A=
		If count>100 Then=0A=
			Exit Sub=0A=
		End If=0A=
		=0A=
		x=3DInt(Rnd*10)+1=0A=
		y=3DInt(Rnd*7)+1=0A=
		r=3DInt(Rnd*2)+1=0A=
	Loop Until Pode(x,y,r,n) =0A=
	Mete x,y,r,n,v=0A=
End Sub=0A=
=0A=
Sub GerarMapa()=0A=
	Dim i,j,x,y,r=0A=
	' limpar o tabuleiro=0A=
	For i=3D1 To 10=0A=
		For j=3D1 To 10=0A=
			tab1(j,i)=3D0=0A=
		Next=0A=
	Next=0A=
=0A=
	res.innerHTML=3D""=0A=
=0A=
	Randomize=0A=
	=0A=
	' gerar o porta-avi=F5es=0A=
	x=3DInt(Rnd*8)+1=0A=
	y=3DInt(Rnd*8)+1=0A=
	r=3DInt(Rnd*4)+1 ' orienta=E7=E3o=0A=
	RodarPA(r)=0A=
	For i=3D0 To 2=0A=
		For j=3D0 To 2=0A=
			tab1(x+j,y+i)=3Dpa(j+1,i+1)=0A=
		Next=0A=
	Next=0A=
	=0A=
	' o resto dos barcos=0A=
	count=3D0=0A=
	Gerar 4,2 ' hospital=0A=
	Gerar 4,3 ' barco de 4 canos=0A=
	Gerar 3,4 ' barco de 3 canos=0A=
	Gerar 3,4 ' barco de 3 canos=0A=
	Gerar 2,5 ' barco de 2 canos=0A=
	Gerar 2,5 ' barco de 2 canos=0A=
	Gerar 2,5 ' barco de 2 canos=0A=
	Gerar 1,6 ' submarino=0A=
	Gerar 1,6 ' submarino=0A=
	Gerar 1,6 ' submarino=0A=
	Gerar 1,6 ' submarino=0A=
	=0A=
	If count>100 Then=0A=
		GerarMapa=0A=
	Else=0A=
		MostrarMapa=0A=
	End If=0A=
	=0A=
	ttiros(0)=3D0=0A=
	ttiros(1)=3D5 ' porta-avi=F5es=0A=
	ttiros(2)=3D4 ' hospital=0A=
	ttiros(3)=3D4 ' barco 4 canos=0A=
	ttiros(4)=3D6 ' barco 3 canos=0A=
	ttiros(5)=3D6 ' barco 4 canos=0A=
	ttiros(6)=3D4 ' submarino=0A=
	For i=3D1 To 10=0A=
		For j=3D1 To 10=0A=
			tab2(j,i)=3Dtab1(j,i)=0A=
		Next=0A=
	Next=0A=
End Sub=0A=
=0A=
Sub MostrarMapa ()=0A=
	For i=3D1 To 10=0A=
		For j=3D1 To 10=0A=
			document.all.item("c"&j&"l"&i).style.background=3D"white"=0A=
'			If tab1(j,i)=3D0 Then=0A=
				document.all.item("c"&j&"l"&i).innerHTML=3D""=0A=
'			Else=0A=
'				document.all.item("c"&j&"l"&i).innerHTML=3Dtab1(j,i)=0A=
'			End If=0A=
		Next=0A=
	Next=0A=
End Sub=0A=
-->=0A=
</SCRIPT>

